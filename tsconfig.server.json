{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "allowJs": true,
      "sourceMap": true,
      "outDir": "./dist",
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
    },
    "include": [
      "src/server/**/*"
    ],
    "exclude": [
      "node_modules",
    ]
  }
  